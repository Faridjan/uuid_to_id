{
  "description": "Transformation UUID to ID",
  "name": "faridjan/uuid_to_id",
  "minimum-stability": "stable",
  "license": "proprietary",
  "config": {
    "sort-packages": true
  },
  "require": {
    "php": ">=7.4.0",
    "ext-json": "*",
