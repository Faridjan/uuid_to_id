{
    "description": "My Framework based from PSR",
    "name": "faridjan/my_framework",
    "minimum-stability": "stable",
    "license": "proprietary",
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": ">=7.4.0",
        "ext-json": "*",
        "php-di/php-di": "^6.3",
        "slim/psr7": "^1.3",
        "slim/slim": "^4.7",
        "symfony/console": "^5.2"
    },
    "require-dev": {
        "overtrue/phplint": "^2.3",
        "roave/security-advisories": "dev-latest",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "App\\Bin\\": "bin/",
            "App\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Farid",
            "email": "fred.andersen7@gmail.com"
        }
    ]
}
